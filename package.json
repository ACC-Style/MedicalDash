{
	"name": "medicaldash",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"build-css": "node-sass src/assets/scss/index.scss public/assets/css/index.css"
	},
	"dependencies": {
		"node-sass": "^4.11.0",
		"sass-loader": "^7.1.0",
		"scss-loader": "0.0.1",
		"vue": "^2.6.10",
		"vue-router": "^3.0.2",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.1",
		"@vue/cli-plugin-eslint": "^3.0.1",
		"@vue/cli-service": "^3.0.1",
		"@vue/eslint-config-prettier": "^3.0.1",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/prettier"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {
			"vue/script-indent": "warning"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
